# Gemini API Key (required for Gemini provider)
GEMINI_API_KEY=your_gemini_api_key_here

# Deepseek API Key (required for Deepseek provider via OpenRouter)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Deepseek Model Name (optional, defaults to deepseek/deepseek-chat-v3-0324:free)
DEEPSEEK_MODEL_NAME=deepseek/deepseek-chat-v3-0324:free
